<%- include("partials/header.ejs") %>
<div class="box">
    <h1 id="heading"><%= nowDate %></h1>
</div>
<div class="box">
        <% allNotes.forEach(function (element) { %>
            <li class="item">
                <input type="checkbox">
                <p><%= element %></p>
            </li>
        <% }) %>

    <form action="/" method="POST" id="form">
        <input type="text" placeholder="New Note" name="newNote" required>
        <button type="submit" name="button" value="submit">+</button>
    </form>
</div>
<%- include("partials/footer.ejs") %>