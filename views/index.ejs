<%- include("partials/header.ejs") %>
<div class="box">
    <h1 id="heading"><%= listTitle %></h1>
</div>
<div class="box">
        <% allNotes.forEach(function (element) { %>
        <form action="/delete" method="POST">
            <li class="item">
                <input type="checkbox" name="checkbox" value="<%= element._id %>" onChange="this.form.submit()">
                <p><%= element.name %></p>
            </li>
            <input type="hidden" name="list" value="Today">
        </form>
        <% }) %>

    <form action="/" method="POST" id="form">
        <input type="text" placeholder="New Note" name="newNote" required>
        <button type="submit" name="list" value="Today">+</button>
    </form>
</div>
<%- include("partials/footer.ejs") %>